    "predeploy": "npm run build",
    "deploy": "now ./build",
    "postdeploy": "now alias -A ./build/now.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "react-test-context-provider": "^2.2.0",
    "react-test-renderer": "^16.8.6"
  }
}
