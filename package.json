{
  "name": "tunechain-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jest-enzyme": "^7.0.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "react-scripts test",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:watch": "jest --watchAll --verbose",
